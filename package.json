{
	"name": "slovko",
	"private": true,
	"version": "0.6.262",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier --write .",
		"deploy": "npm run build && npx gh-pages -d build -t true",
		"fix:b2": "node scripts/maintenance/fix_b2_gaps_from_other_levels.js",
		"check:missing": "node scripts/analysis/find_missing_all.js",
		"check:missing:full": "node scripts/analysis/find_missing_comprehensive.js",
		"check:duplicates": "node scripts/analysis/check_duplicates.js",
		"analyze:duplicates:global": "node scripts/analysis/check_global_duplicates.js",
		"check:transcriptions": "node scripts/analysis/check_transcriptions.js",
		"analyze:vocab": "node scripts/analysis/vocab_analysis.js",
		"i18n:check": "node scripts/maintenance/check_translations.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"svelte-eslint-parser": "^1.4.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0"
	},
	"dependencies": {
		"firebase": "^12.8.0",
		"glob": "^13.0.0",
		"lucide-svelte": "^0.563.0",
		"svelte-i18n": "^4.0.1",
		"zod": "^4.3.6"
	},
	"overrides": {
		"cookie": "^0.7.2",
		"esbuild": "^0.25.0"
	}
}
